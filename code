#include<iostream>
#include<conio.h>
#include<stdio.h>
using namespace std;
class student
{
	public:

	int id[3]; //ID number of student
    int BT[3]; //Burst Time	
    int CT[3]; //Completon Time
	int FT[3]; //Food taken Time
  	
  	bool check( )
{
    for (int i=0;i<3;i++){
        if(FT[i]!=0){
            return true;
        }
    }
    return false;
}
