  	bool check( )
{
    for (int i=0;i<3;i++){
        if(FT[i]!=0){
            return true;
        }
    }
    return false;
}
   void run()
   {		
	for( int i=0;i<3;i++){		
        cout<<"Enter the ID number of student "<<i+1<<endl;
        cin>>id[i];
        cout<<"Enter the food taken time by the Student "<<i+1<<endl;
        cin>>BT[i];
      	BT[i]=FT[i];

	}
